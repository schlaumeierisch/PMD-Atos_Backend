<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping" default-access="field">
    <class name="nl.hva.backend.domain.MedicalRecord" table="medical_record">

        <id name="id" column="id" type="long">
            <generator class="increment"/>
        </id>

        <component name="domainId" class="nl.hva.backend.domain.ids.MedicalRecordId">
            <property name="id" column="medical_record_id" type="string"/>
        </component>

        <component name="patientDomainId" class="nl.hva.backend.domain.ids.PatientId">
            <property name="id" column="patient_id" type="string"/>
        </component>

    </class>
</hibernate-mapping>